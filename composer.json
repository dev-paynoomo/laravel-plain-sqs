{
    "name": "dev-paynoomo/laravel-plain-sqs",
    "type": "library",
    "description": "Custom SQS connector for Laravel that supports custom format JSON",
    "keywords": [
        "php",
        "laravel",
        "sqs",
        "aws"
    ],
    "homepage": "https://github.com/dev-paynoomo/laravel-plain-sqs",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0",
        "illuminate/queue": "^12.0",
        "illuminate/bus": "^12.0",
        "aws/aws-sdk-php": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12"
    },
    "autoload": {
        "psr-4": {
            "Dusterio\\PlainSqs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dusterio\\PlainSqs\\Integrations\\LaravelServiceProvider"
            ]
        }
    }
}
